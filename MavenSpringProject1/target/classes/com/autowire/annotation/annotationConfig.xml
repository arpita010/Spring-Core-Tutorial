<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-3.0.xsd">

<context:annotation-config/>
<bean class="com.autowire.annotation.Project" name="p1">
<property name="pid" value="11" />
<property name="pName" value="assignment" />
</bean>
<bean class="com.autowire.annotation.Project" name="p2">
<property name="pid" value="18" />
<property name="pName" value="assignment 3" />
</bean>
<bean class="com.autowire.annotation.Employee" name="e1">
<property name="eid" value="159" />
<property name="eName" value="teacher" />

</bean>
</beans>